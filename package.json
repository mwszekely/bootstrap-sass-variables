{
  "name": "bootstrap-sass-variables",
  "version": "0.0.1",
  "description": "Makes it easier to @use customized Bootstrap variables with no redundancies",
  "scripts": {
    "build": "sass -I='./node_modules' ./demo-dark.scss ./demo-dark.css",
    "watch": "sass -I='./node_modules' ./demo-dark.scss ./demo-dark.css -w"
  },
  "peerDependencies": {
    "bootstrap": "^5.1.3"
  },
  "devDependencies": {
    "sass": "^1.49.0"
  }
}
